{
    "name": "restaurangmenyn",
    
    "dependencies": {
        "@vue/babel-helper-vue-jsx-merge-props": "1.0.0",
        "@vue/babel-preset-jsx": "1.1.2",
        "babel-plugin-jsx-vue-functional": "2.1.0",
        "babel-preset-es2015": "6.24.1"
        },
    "devDependencies": {
        "sass": "^1.51.0",
        "nodemon": "^2.0.16",
        "npm-run-all": "^4.1.5",
        "copyfiles": "^2.4.1",
        "vue": "3.2.33",
        "vue-loader": "17.0.0",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "vue-style-loader": "4.1.3",
        "css-loader": "6.7.1",
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/preset-env": "^7.17.10",
        "@babel/plugin-transform-runtime": "^7.17.10",
        "babel-loader": "^8.2.5",
        "babel-plugin-transform-vue-jsx": "3.7.0",
        "babel-plugin-syntax-jsx": "6.18.0",
        "@vue/babel-plugin-jsx": "1.1.1",
        "vuex": "4.0.2",
        "ws": "8.6.0",
        "express": "4.18.1",
        "cors": "2.8.5"
    },
    "scripts": {
        "start": "nodemon ./src/server/server.mjs",
        "watch": "nodemon --exec \"npm run build\"",
        "build:sass": "sass --no-source-map ./src/sass:./public/css",
        "copy:assets": "copyfiles -u 1 ./src/assets/**/* ./public",
        "copy:vue": "copyfiles -u 1 ./src/vue/**/* ./public",
        "copy:index": "copyfiles -u 1 ./src/index.html ./public",
        "copy": "npm-run-all --parallel copy:*",
        "webpack": "webpack",
        "build": "npm-run-all webpack copy build:* start"
    },
    "nodemonConfig": {
        "watch": [
            "./src/*"
        ],
        "ext": "js,scss,html",
        "ignore": [
            "public/*"
        ],
        "delay": 2500
    }

}